-- Checking if the PlaceId condition is satisfied
if game.PlaceId == 15744137588 then

    -- Loading the OrionLib library
    local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
    if not OrionLib then
        error("Failed to load the OrionLib library")
        return
    end

    -- Creating the main window
    local Window = OrionLib:MakeWindow({
        Name = "Trade+Dupe Script",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "WendelCfg",
        IntroEnabled = false
    })

    -- Table to store global variables
    local G = {}

    -- Creating the Player tab
    local RollerTab = Window:MakeTab({
        Name = "Roll-Tab",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    local RerollButton = RollerTab:AddButton({
        Name = "Reroll Mask",
        Callback = function()
            local FoundReroll = false

            repeat
                local args = {
                    [1] = 400001
                }

                local reroll = game:GetService("ReplicatedStorage").Remotes.RerollOrnament:InvokeServer(unpack(args))
                wait(.1)
                if tostring(reroll.ornamentId) ~= "410001" and
                    tostring(reroll.ornamentId) ~= "410002" and
                    tostring(reroll.ornamentId) ~= "410003" and
                    tostring(reroll.ornamentId) ~= "410004" and
                    tostring(reroll.ornamentId) ~= "410005" then
                    print("Unknown reroll found ! : " .. tostring(reroll.ornamentId))
                    FoundReroll = true
                end

                if tostring(reroll.ornamentId) == "410004" or
                    tostring(reroll.ornamentId) == "410005" then
                    local rerollname = "None"
                    if tostring(reroll.ornamentId) == "410004" then
                        rerollname = "Legendary"
                    elseif tostring(reroll.ornamentId) == "410005" then
                        rerollname = "Rainbow"
                    end
                    print("!! REROLL FOUND !! : " .. rerollname)
                    FoundReroll = true
                end
            until FoundReroll
            print("Script stopped !")
        end
    })


local RerollButton = RollerTab:AddButton({
        Name = "Reroll Awakening",
        Callback = function()
            local FoundReroll = false

            repeat
                local args = {
                    [1] = 400002
                }

                local reroll = game:GetService("ReplicatedStorage").Remotes.RerollOrnament:InvokeServer(unpack(args))
                wait(.1)
                if tostring(reroll.ornamentId) ~= "410006" and
                    tostring(reroll.ornamentId) ~= "410007" and
                    tostring(reroll.ornamentId) ~= "410008" and
                    tostring(reroll.ornamentId) ~= "410009" and
                    tostring(reroll.ornamentId) ~= "410010" then
                    print("Unknown reroll found ! : " .. tostring(reroll.ornamentId))
